# Example .env for dockerized deployment
# Copy to .env and fill values before running `docker compose up`

# Django
DJANGO_DEBUG=False
DJANGO_SECRET=replace-with-a-long-random-secret
DJANGO_SETTINGS_MODULE=soundbox.settings
ALLOWED_HOSTS=141.11.123.238,your.domain.com

# Postgres (host.docker.internal allows containers to reach the host's postgres)
DB_NAME=soundbox_db
DB_USER=soundbox_user
DB_PASSWORD=your_db_password
DB_HOST=host.docker.internal
DB_PORT=5432
